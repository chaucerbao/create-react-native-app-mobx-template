{
  "name": "react-native-app",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-concentric-order"
    ],
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "syntax": "scss"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "styled-components-stylefmt --recursive",
      "eslint",
      "stylelint",
      "git add"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "expo": "^16.0.0",
    "husky": "^0.13.3",
    "jest-expo": "^0.4.0",
    "lint-staged": "^3.4.1",
    "prettier-eslint-cli": "^3.4.4",
    "react-native-scripts": "0.0.29",
    "react-test-renderer": "16.0.0-alpha.6",
    "styled-components-stylefmt": "^0.1.2",
    "stylelint": "^7.10.1",
    "stylelint-config-concentric-order": "^2.0.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0"
  },
  "dependencies": {
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.4",
    "react-router-native": "^4.1.1",
    "styled-components": "^1.4.6"
  },
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "precommit": "lint-staged"
  }
}
